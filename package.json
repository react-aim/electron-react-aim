    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.99.1",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.2",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^4.0.0-rc.12",
    "jest": "^24.8.0",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "mathjs": "^5.10.3",
    "mini-css-extract-plugin": "^0.7.0",

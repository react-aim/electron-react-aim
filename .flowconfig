[ignore]
<PROJECT_ROOT>/app/main.prod.js
<PROJECT_ROOT>/app/main.prod.js.map
<PROJECT_ROOT>/app/dist/.*
<PROJECT_ROOT>/resources/.*
<PROJECT_ROOT>/node_modules/webpack-cli
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/dll/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/git/.*

[include]

[libs]

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='styl' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/internals/flow/CSSModule.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/internals/flow/WebpackAsset.js.flow'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/internals/flow/WebpackAsset.js.flow'
module.name_mapper='.*\(.s?css\)' -> 'css-module-flow'
module.name_mapper='^@/' -> '<PROJECT_ROOT>/app/'
module.name_mapper='^@comp' -> '<PROJECT_ROOT>/app/components'
module.name_mapper='^@common/' -> '<PROJECT_ROOT>/app/common'
module.name_mapper='^@commonComp' -> '<PROJECT_ROOT>/app/common/components'
module.name_mapper='^@commonScss' -> '<PROJECT_ROOT>/app/common/scss'
module.name_mapper='^@commonStore' -> '<PROJECT_ROOT>/app/common/store'
module.name_mapper='^@commonStyled' -> '<PROJECT_ROOT>/app/common/styled'
module.name_mapper='^@con' -> '<PROJECT_ROOT>/app/containers'
module.name_mapper='^@store' -> '<PROJECT_ROOT>/app/store'
module.name_mapper='^@scss' -> '<PROJECT_ROOT>/app/scss'
module.name_mapper='^@styled' -> '<PROJECT_ROOT>/app/container/styled'
module.name_mapper='^@util' -> '<PROJECT_ROOT>/app/util'
module.name_mapper='^@tool' -> '<PROJECT_ROOT>/app/util/tool'
module.name_mapper='^@lib' -> '<PROJECT_ROOT>/app/util/lib'
module.name_mapper='^@static' -> '<PROJECT_ROOT>/app/static'
module.name_mapper='^@icon' -> '<PROJECT_ROOT>/app/static/icon'
module.name_mapper='^@image' -> '<PROJECT_ROOT>/app/static/image'
module.name_mapper='^@picture' -> '<PROJECT_ROOT>/app/static/picture'
module.name_mapper='^@music' -> '<PROJECT_ROOT>/app/static/music'
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
